{
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/api/:path*"
        },
        {
            "source": "/((?!api|@vite|@react-refresh|_vercel|src|node_modules|public|manifest.webmanifest|.*\\.(?:jpg|jpeg|png|gif|svg|ico|css|js)).*)",
            "destination": "/index.html"
        }
    ]
}